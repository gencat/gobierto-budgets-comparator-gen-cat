<script>
function CompartirTwitter(){
  var sharerTitle = document.title.substr(-23);
  var sharerDesc = document.title.slice(0, -26);

	var width  = 575,
    height = 400,
    left   = ($(window).width()  - width)  / 2,
    top    = ($(window).height() - height) / 2,
    url    = 'http://twitter.com/share?text=',
    opts   = 'status=1' +
             ',width='  + width  +
             ',height=' + height +
             ',top='    + top    +
             ',left='   + left;
	url += encodeURIComponent('A @governobertcat ' + sharerTitle + ': ' + sharerDesc);
	url +=' ';

	window.open(url, 'Twitter', opts);
}

function CompartirFacebook(){
	var width  = 575,
    height = 400,
    left   = ($(window).width()  - width)  / 2,
    top    = ($(window).height() - height) / 2,
    url    = 'https://www.facebook.com/sharer/sharer.php?u=',
    opts   = 'status=1' +
             ',width='  + width  +
             ',height=' + height +
             ',top='    + top    +
             ',left='   + left;

	url+= encodeURIComponent(document.URL);
	window.open(url, 'Facebook', opts);
}
</script>
